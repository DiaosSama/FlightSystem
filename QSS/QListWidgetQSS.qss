QListWidget, QListView, QTreeWidget, QTreeView {
    outline: 0px;
	border: 0px;
}

QListWidget {
    min-width: 95px;
    max-width: 95px;
    color: Black;
    background: #FAFAFA;
}

QListWidget::Item:selected {
    background: lightGray;
    border-left: 5px solid #1874CD;
}
HistoryPanel:hover {
    background: rgb(52, 52, 52);
}